<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, ©tial-scale=1.0">
    <title>Job for change</title>
    <meta name="description" content="Version bêta à venir : inscrivez-vous !">
    <meta name="author" content="">
    <meta property="og:url" content="http://jobforchange.org" />
    <meta property="og:title" content="JobForChange.org" />
    <meta property="og:image" content="http://jobforchange.org/logo.png" />
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>
  </head>
  <body>
    
    <div id="main" role="main">
      <div class="container">
        <div class="content">
           <div class="row">
            <div class="span12">
              <%= render 'layouts/messages' %>
              <%= yield %>
            </div>
          </div>
          <br><br><hr>
          <footer id="footer">
            <%= render 'layouts/footer' %>
          </footer>
        </div>
      </div> <!--! end of .container -->
    </div> <!--! end of #main -->
  </body>
</html>